<?xml version="1.0"?>
<block>
  <name>GRPC Sink</name>
  <key>grpc_blocks_sink</key>
  <category>[grpc_blocks]</category>
  <import>from grpc_blocks import grpc_blocks_swig as grpcsink</import>
  <make>grpcsink.sink()</make>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
</block>
